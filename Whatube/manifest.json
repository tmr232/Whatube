{
  "name": "Whatube",
  "version": "1.0",
  "description": "Show the names of videos in youtube links.",

  "permissions": ["http://www.youtube.com/watch?*",
                  "https://www.youtube.com/watch?*"],

  "background": {
    "scripts": ["jquery-2.0.2.min.js", "background.js"]
  },

  "content_scripts" : [
        {
            "matches": ["https://www.facebook.com/*"],
            "js":["jquery-2.0.2.min.js", "mutation_summary.js", "content.js"],
            "all_frames": true
            }
    ],

  "manifest_version": 2
}